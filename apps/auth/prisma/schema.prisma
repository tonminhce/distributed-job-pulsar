generator client {
    provider = "prisma-client-js"
    binaryTargets = ["native", "debian-openssl-1.1.x"]
    output = "../../../node_modules/@prisma-clients/auth"
}

datasource db {
    provider = "postgresql"
    url = env("AUTH_DATABASE_URL")
}

model User {
    id       String   @id @default(uuid())
    email    String   @unique
    name     String
    password String
}